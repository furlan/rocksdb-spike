    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/examples/LowLevel/bin/Debug/net9.0/LowLevel.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "C#: ModestBlackwell Debug",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/modest-blackwell/ModestBlackwell.csproj",
            "preLaunchTask": "run-debug"
        },
        {
            "name": "C#: ModestBlackwell Debug (No Build)",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/modest-blackwell/ModestBlackwell.csproj"
        },
        {
            "name": "C#: ModestBlackwell Development Debug",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/modest-blackwell/ModestBlackwell.csproj",
            "preLaunchTask": "build"
        },
        {
            "name": "C#: ModestBlackwell Watch Debug",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/modest-blackwell/ModestBlackwell.csproj",
            "preLaunchTask": "watch"
        }
    ]
}